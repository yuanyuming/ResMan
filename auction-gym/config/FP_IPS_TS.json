{
  "random_seed": 0,
  "num_runs": 2,
  "num_iter" : 3,
  "rounds_per_iter": 10000,
  "num_participants_per_round": 2,
  "embedding_size": 5,
  "embedding_var": 1.0,
  "obs_embedding_size": 4,
  "allocation": "FirstPrice",
  "agents": [ {
    "name": "IPS",
    "num_copies": 3,
    "num_items": 12,
      "allocator": {
        "type": "PyTorchLogisticRegressionAllocator",
        "kwargs": {"embedding_size": 4, "num_items": 12}
      },
      "bidder": {
          "type": "PolicyLearningBidder",
          "kwargs": {
            "gamma_sigma":  0.02,
            "init_gamma": 1.0,
            "loss": "\"PPO\""
          }
    }
  }],
  "output_dir": "results/FP_IPS_TS/"
}
